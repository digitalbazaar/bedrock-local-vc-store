{
  "name": "bedrock-local-vc-store-test",
  "version": "0.0.1-0",
  "private": true,
  "scripts": {
    "test": "mocha -r esm --preserve-symlinks -t 30000 -A -R ${REPORTER:-spec} --require test-mocha.js *.spec.js",
    "test-karma": "karma start ../karma.conf.js",
    "test-watch": "mocha -r esm --watch --preserve-symlinks -t 30000 -A -R ${REPORTER:-spec} --require test-mocha.js *.spec.js",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test-node",
    "coverage-ci": "nyc --reporter=lcovonly npm run test-node",
    "coverage-report": "nyc report",
    "lint": "eslint ."
  },
  "dependencies": {
    "bedrock": "^3.0.0",
    "bedrock-karma": "^2.1.0",
    "bedrock-local-vc-store": "file:..",
    "bedrock-test": "^5.0.0"
  }
}
